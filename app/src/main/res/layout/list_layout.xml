<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aandroid="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="15sp"
    android:layout_marginStart="15dp"
    aandroid:background="@drawable/rounded"
    android:elevation="2sp"
    android:orientation="horizontal">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layout"
        android:layout_width="match_parent"
        android:layout_height="123dp"
        android:background="@drawable/rounded"
        android:clickable="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="67dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layoutchange"
            android:layout_width="15dp"
            android:layout_height="122dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/Name2"
            android:layout_width="212dp"
            android:layout_height="33dp"
            android:layout_marginTop="12dp"
            android:ellipsize="end"
            android:maxLines="1"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@+id/imageView"
            app:layout_constraintTop_toBottomOf="@+id/Sarifle2" />

        <TextView
            android:id="@+id/Sarifle2"
            android:layout_width="178dp"
            android:layout_height="33dp"
            android:layout_marginStart="1dp"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@+id/imageView"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/Accountnum2"
            android:layout_width="164dp"
            android:layout_height="28dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="17dp"
            android:ems="10"
            android:textColor="#000000"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView"
            app:layout_constraintTop_toBottomOf="@+id/Name2"
            app:layout_constraintVertical_bias="0.0" />

<!--        <ImageButton-->
<!--            android:id="@+id/DeleteId"-->
<!--            android:layout_width="45dp"-->
<!--            android:layout_height="47dp"-->
<!--            android:layout_marginTop="41dp"-->
<!--            android:layout_marginEnd="16dp"-->
<!--            android:layout_marginBottom="45dp"-->
<!--            android:backgroundTint="#006A3838"-->
<!--            android:contentDescription="TODO"-->
<!--            app:layout_constraintBottom_toBottomOf="parent"-->
<!--            app:layout_constraintEnd_toEndOf="parent"-->
<!--            app:layout_constraintTop_toTopOf="parent"-->
<!--            app:srcCompat="@android:drawable/ic_delete"-->
<!--            app:tint="@color/light_teal" />-->

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="69dp"
            android:layout_height="83dp"
            android:layout_marginStart="2dp"
            android:layout_marginTop="24dp"
            android:contentDescription="TODO"
            app:layout_constraintStart_toEndOf="@+id/layoutchange"
            app:layout_constraintTop_toTopOf="parent"
            tools:srcCompat="@drawable/iconuser" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>